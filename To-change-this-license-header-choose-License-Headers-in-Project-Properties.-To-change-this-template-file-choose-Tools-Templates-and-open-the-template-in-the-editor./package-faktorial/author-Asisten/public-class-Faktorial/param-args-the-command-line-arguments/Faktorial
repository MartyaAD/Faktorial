package faktorial;

/**
 *
 * @author Asisten
 */
public class Faktorial {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int y,z;
        int input=5;
        int x =1;
        System.out.print("input : ");
        System.out.println(input);
        for(int i=1;i<=input;i++){
            x=i*x;
        }
        System.out.println(x);
            
        }
    }
    
